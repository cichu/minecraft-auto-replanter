{
  "schemaVersion": 1,
  "id": "auto_replanter",
  "version": "${version}",
  "name": "Auto replanter",
  "description": "Minecraft mod for harvesting and auto replanting crops.",
  "authors": [
    "cichu"
  ],
  "contact": {
    "homepage": "https://github.com/cichu/minecraft-auto-replanter",
    "issues": "https://github.com/cichu/minecraft-auto-replanter/issues",
    "sources": "https://github.com/cichu/minecraft-auto-replanter"
  },
  "license": "MIT license",
  "icon": "assets/auto_replanter/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.github.cichu.auto_replanter.AutoReplanter"
      }
    ]
  },
  "mixins": [
    "auto_replanter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
